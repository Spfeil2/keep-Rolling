{
  "name": "keep-rolling",
  "displayName": "keep rolling",
  "version": "1.0.0",
  "description": "keep rolling is a location based service and reporting system for tracking bicycle obstructions in urban areas",
  "main": "index.js",
  "keywords": [
    "location based service",
    "lbs",
    "cordova"    
  ],
  "devDependencies": {
    "cordova-android": "^9.1.0",
    "cordova-browser": "^6.0.0",
    "cordova-plugin-camera": "^5.0.3",
    "cordova-plugin-cleartext": "^1.0.0",
    "cordova-plugin-geolocation": "^4.1.0",
    "cordova-plugin-whitelist": "^1.3.4"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-geolocation": {
        "GPS_REQUIRED": "true"
      },
      "cordova-plugin-camera": {}
    },
    "platforms": [
      "browser",
      "android"
    ]
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1"
  }
}
